
check_machine_image()
{
    if [ "$onie_build_machine" != "$image_build_machine" ] &&
       [ "accton_$onie_build_machine" != "$image_build_machine" ] ; then
        fail=yes
    fi
    if [ "$onie_machine_rev" != "$image_machine_rev" ] ; then
        fail=yes
    fi
    if [ "$onie_arch" != "$image_arch" ] ; then
        fail=yes
    fi
}
